{
  "name": "angular2-starter",
  "version": "0.9.1",
  "description": "A starter kit for Angular2+Rxjs UIs using Webpack",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "pretest": "npm run clean:test",
    "test": "cross-env NODE_ENV=test karma start",
    "posttest": "npm run cover:html && npm run cover:lcov",
    "cover:html": "remap-istanbul --input ./coverage/json/coverage-final.json --output ./coverage/html --type html",
    "cover:lcov": "remap-istanbul --input ./coverage/json/coverage-final.json --output ./coverage/json/lcov.info --type lcovonly",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors --bail",
    "serve": "cross-env NODE_ENV=production node server.js",
    "clean:build": "rimraf dist",
    "clean:doc": "rimraf doc",
    "clean:test": "rimraf coverage",
    "predoc": "npm run clean:doc",
    "doc": "typedoc --options typedoc.json --exclude \"**/*.+(spec|e2e).ts\" ./src/app",
    "webdriver-update": "webdriver-manager update",
    "pree2e": "npm run webdriver-update",
    "e2e": "protractor",
    "rimraf": "rimraf",
    "cross-env": "cross-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/angular2-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/angular2-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/angular2-starter#readme",
  "private": true,
  "devDependencies": {
    "@ngrx/store-devtools": "3.2.2",
    "@ngrx/store-log-monitor": "3.0.2",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.51",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "0.0.28",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-nyan-reporter": "0.2.4",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "3.13.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.13",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.1.1",
    "protractor": "4.0.11",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.7.0",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "4.0.2",
    "sasslint-webpack-plugin": "1.0.4",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-node": "1.7.0",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typedoc": "0.5.1",
    "typescript": "2.0.10",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-merge": "1.0.2"
  },
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "2.2.4",
    "@angular/http": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/router": "3.2.4",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.1",
    "core-js": "2.4.1",
    "koa": "1.2.4",
    "koa-compress": "1.0.9",
    "koa-favicon": "1.2.1",
    "koa-logger": "1.3.0",
    "koa-send": "3.2.0",
    "koa-static": "2.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  }
}
