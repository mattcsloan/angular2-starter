{
  "name": "angular2-starter",
  "version": "0.4.0",
  "description": "A starter kit for Angular2+Rxjs UIs using Webpack",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "pretest": "npm run clean:cov",
    "test": "karma start",
    "posttest": "npm run cover:html && npm run cover:lcov",
    "cover:html": "remap-istanbul --input ./coverage/json/coverage-final.json --output ./coverage/html --type html",
    "cover:lcov": "remap-istanbul --input ./coverage/json/coverage-final.json --output ./coverage/json/lcov.info --type lcovonly",
    "prebuild": "npm run clean:dist",
    "build": "webpack --config webpack/webpack.prod.js --progress --profile --bail",
    "serve": "http-server ./dist",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf doc",
    "clean:cov": "rimraf coverage",
    "clean:typings": "rimraf typings",
    "postinstall": "typings install",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "tslint \"./src/**/*.ts\" --force --format verbose",
    "lint:css": "sass-lint \"src/**/*.scss\" --verbose",
    "wrap": "npm shrinkwrap --dev",
    "typings": "typings",
    "predocs": "npm run clean:docs",
    "docs": "typedoc --options typedoc.json --exclude \"**/*.spec.ts\" ./src/",
    "webdriver-update": "webdriver-manager update",
    "pree2e": "npm run webdriver-update",
    "e2e": "protractor",
    "rimraf": "rimraf",
    "cpy": "cpy",
    "mkdirp": "mkdirp",
    "validate-webpack-dev": "webpack-validator ./webpack/webpack.dev.js",
    "validate-webpack-prod": "webpack-validator ./webpack/webpack.prod.js",
    "validate-webpack-test": "webpack-validator ./webpack/webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/angular2-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/angular2-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/angular2-starter#readme",
  "private": true,
  "devDependencies": {
    "awesome-typescript-loader": "1.1.1",
    "codelyzer": "0.0.28",
    "cpy": "4.0.1",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mkdirp": "0.5.1",
    "node-sass": "3.8.0",
    "null-loader": "0.1.1",
    "postcss-cssnext": "2.7.0",
    "postcss-loader": "0.10.1",
    "protractor": "4.0.3",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.6.4",
    "rimraf": "2.5.4",
    "sass-lint": "1.9.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.4",
    "ts-node": "1.3.0",
    "tslint": "3.14.0",
    "typedoc": "0.4.5",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.1",
    "webpack-validator": "2.2.7"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular2-material/button": "2.0.0-alpha.7-4",
    "@angular2-material/core": "2.0.0-alpha.7-4",
    "@angular2-material/icon": "2.0.0-alpha.7-4",
    "@angular2-material/sidenav": "2.0.0-alpha.7-4",
    "@angular2-material/toolbar": "2.0.0-alpha.7-4",
    "angular2-in-memory-web-api": "0.0.7",
    "es6-shim": "0.35.0",
    "normalize.css": "4.2.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "engines": {
    "node": "6.4.0",
    "npm": "3.10.3"
  }
}
