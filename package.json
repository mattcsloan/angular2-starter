  "bugs": {
    "url": "https://github.com/ecozoic/angular2-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/angular2-starter#readme",
  "devDependencies": {
    "@ngrx/devtools": "1.3.3",
    "codelyzer": "0.0.19",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "html-webpack-plugin": "2.16.1",
    "node-sass": "3.7.0",
    "postcss-cssnext": "2.5.2",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "sass-lint": "1.7.0",
