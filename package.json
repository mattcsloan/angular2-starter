{
  "name": "angular2-starter",
  "version": "0.0.1",
  "description": "A starter kit for Angular2+Rxjs UIs",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "postinstall": "typings install",
    "build": "npm run clean && cross-env NODE_ENV=development webpack",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "lint:js": "tslint './src/**/*.ts'",
    "lint:css": "sass-lint src/**/*.scss",
    "wrap": "npm shrinkwrap --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/angular2-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
