    "@ngrx/store": "1.3.3",
    "angular2": "2.0.0-beta.17",
    "es5-shim": "4.5.8",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
