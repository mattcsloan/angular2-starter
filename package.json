{
  "name": "angular2-starter",
  "version": "0.0.1",
  "description": "A starter kit for Angular2+Rxjs UIs using Webpack",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "npm run clean && webpack --config webpack/webpack.prod.js --progress --profile --bail",
    "clean": "rimraf dist",
    "postinstall": "typings install",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "tslint './src/**/*.ts'",
    "lint:css": "sass-lint src/**/*.scss",
    "wrap": "npm shrinkwrap --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecozoic/angular2-starter.git"
  },
  "author": "John Mitchell <johnnn.mitchell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecozoic/angular2-starter/issues"
  },
  "homepage": "https://github.com/ecozoic/angular2-starter#readme",
  "devDependencies": {
    "awesome-typescript-loader": "0.17.0",
    "codelyzer": "0.0.19",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.17.0",
    "jasmine-core": "2.4.1",
    "karma": "0.13.22",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "3.7.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-cssnext": "2.5.2",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "sass-lint": "1.7.0",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.4",
    "tslint": "3.10.2",
    "typescript": "1.8.10",
    "typings": "1.0.4",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.12.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  }
}
